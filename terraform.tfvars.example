# Example Terraform variables file

# AWS Region (EU regions for your requirement)
aws_region = "eu-central-1"

# Cluster name (must be unique in your AWS account)
cluster_name = "minimal-eks-cluster"

# Optional: EC2 Key Pair for SSH access to nodes
# Create one first: aws ec2 create-key-pair --key-name my-key
# key_pair_name = "my-key-pair"

# Environment tag
environment = "dev"